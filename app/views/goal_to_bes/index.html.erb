<h1>Listing goal_to_bes</h1>

<table>
  <thead>
    <tr>
      <th>Users</th>
      <th>Dream</th>
      <th>Goal</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goal_to_bes.each do |goal_to_be| %>
      <tr>
        <td><%= goal_to_be.users_id %></td>
        <td><%= goal_to_be.dream %></td>
        <td><%= goal_to_be.goal %></td>
        <td><%= goal_to_be.date %></td>
        <td><%= link_to 'Show', goal_to_be %></td>
        <td><%= link_to 'Edit', edit_goal_to_be_path(goal_to_be) %></td>
        <td><%= link_to 'Destroy', goal_to_be, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Goal to be', new_goal_to_be_path %>
